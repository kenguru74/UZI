<game_task id="work_on_topol">
   <title>Узнать о судьбе Усача</title>
   <objective>
      <text>Сообщить о судьбе Усача</text>
      <icon>ui_iconsTotal_kill_stalker</icon>
      <infoportion_complete>second_dialog_topol</infoportion_complete>
      <infoportion_fail>usach_death</infoportion_fail>
   </objective>

   <objective>
      <text>Обследовать район падения беспилотника</text>
      <infoportion_complete>dialog_usach</infoportion_complete>
      <infoportion_fail>usach_death</infoportion_fail>
   </objective>

   <objective>
      <text>Включить аварийный радиомаяк</text>
      <infoportion_complete>send_message_german</infoportion_complete>
      <infoportion_fail>usach_death</infoportion_fail>
   </objective>

   <objective>
      <text>Получить сообщение Германа</text>
      <infoportion_complete>take_message_german</infoportion_complete>
      <infoportion_fail>usach_death</infoportion_fail>
   </objective>

   <objective>
      <text>Дождаться прилёта вертушки</text>
      <infoportion_complete>heli_move</infoportion_complete>
      <infoportion_fail>usach_death</infoportion_fail>
   </objective>

   <objective>
      <text>Поговорить с Усачом</text>
      <infoportion_complete>dialog_usach2</infoportion_complete>
      <infoportion_fail>usach_death</infoportion_fail>
   </objective>

   <objective>
      <text>Поискать информацию у учёного</text>
      <function_complete>jupiter_dialog.flash_ecolog_have</function_complete>
      <infoportion_set_complete>give_ecolog_info</infoportion_set_complete>
      <infoportion_fail>usach_death</infoportion_fail>
   </objective>

   <objective>
      <text>Найти и обыскать тайники в Госпитале</text>
      <function_complete>jupiter_dialog.artefacts_have</function_complete>
      <infoportion_set_complete>give_german_arts</infoportion_set_complete>
      <infoportion_fail>usach_death</infoportion_fail>
   </objective>

   <objective>
      <text>Вернуться к Герману</text>
      <map_location_type hint="Герман">blue_location</map_location_type>
      <object_story_id>jupiter_german</object_story_id>
      <infoportion_complete>dialog_quest_german</infoportion_complete>
      <infoportion_fail>usach_death</infoportion_fail>
   </objective>

   <objective>
      <text>Сообщить о судьбе Усача</text>
      <map_location_type hint="Тополь">blue_location</map_location_type>
      <object_story_id>jupiter_topol</object_story_id>
      <infoportion_complete>second_dialog_topol</infoportion_complete>
   </objective>
</game_task>

