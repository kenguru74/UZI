<game_task id="work_on_german">
   <title>Работа на Германа</title>
   <objective>
      <text>Раскрыть тайну лаборатории</text>
      <icon>ui_iconsTotal_yan_scientist_job</icon>
      <infoportion_complete>known_secret_labs</infoportion_complete>
   </objective>

   <objective>
      <text>Найти документацию в секретной лаборатории</text>
