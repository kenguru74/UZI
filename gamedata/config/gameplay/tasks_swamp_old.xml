<game_task id="know_for_fate_of_klyk">
   <title>Узнать о судьбе Клыка</title>
   <objective>
      <text>Поговорить с Доктором</text>
      <icon>ui_iconsTotal_kill_stalker</icon>
      <infoportion_complete>known_of_klyk</infoportion_complete>
   </objective>

   <objective>
      <text>Поискать информацию в тайнике</text>
      <function_complete>l03u_agr_underground_dialog.books_research_have</function_complete>
      <infoportion_set_complete>talk_to_doctor_1</infoportion_set_complete>
   </objective>

   <objective>
      <text>Принести информацию Доктору</text>
      <infoportion_complete>bring_doctor_klyk_zapiska1</infoportion_complete>
   </objective>

   <objective>
