
<game_task id="pred_trader_find_weapon">
   <title>Найти винтовку FN F2000</title>
   <objective>
      <text>Найти винтовку FN F2000</text>
      <icon>ui_iconsTotal_find_item</icon>
      <infoportion_complete>bring_weapon</infoportion_complete>
   </objective>

   <objective>
      <text>Найти винтовку FN F2000</text>
      <icon height="50" width="50" x="100" y="50">ui\ui_icons_task</icon>
      <map_location_type hint="Найти предмет">find_item_location</map_location_type>
      <object_story_id>warlab_rukzak</object_story_id>
      <function_complete>predbannik_dialog.weapon_have</function_complete>
      <infoportion_set_complete>find_weapon</infoportion_set_complete>
   </objective>

   <objective>
      <text>Принести винтовку Скупому</text>
      <map_location_type hint="Скупой">blue_location</map_location_type>
      <object_story_id>predbannik_trader</object_story_id>
      <infoportion_complete>bring_weapon</infoportion_complete>
   </objective>
</game_task>


