[logic]
active = ph_door@locked

[ph_door@locked]
closed = true
locked = true
snd_open_start = trader_door_open_start
snd_close_start = trader_door_close_start
snd_close_stop = trader_door_close_stop
show_tips = false
on_info = {+steel_forest_lab_door_open +find_lab} ph_door@unlocked, {-steel_forest_lab_door_open +find_lab} ph_door@locked

[ph_door@unlocked]
closed = false
locked = true
snd_open_start = trader_door_open_start
snd_close_start = trader_door_close_start
snd_close_stop = trader_door_close_stop
show_tips = false
on_info = {-steel_forest_lab_door_open +find_lab} ph_door@locked, {+steel_forest_lab_door_open +find_lab} ph_door@unlocked