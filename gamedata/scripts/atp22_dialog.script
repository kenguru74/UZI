end

function take_reward(actor, npc)
	dialogs.relocate_money(actor, 3000, "in")
end

function give_gausspistol(first_speaker, second_speaker)
	dialogs.relocate_item_section(first_speaker, "wpn_gungauss", "out")
		  db.actor:set_character_rank(db.actor:character_rank() + 30)
end

function actor_have_gungauss(first_speaker, second_speaker)
	return first_speaker:object("wpn_gungauss") ~= nil
end