function checkStalker(npc_community, gulag_type, npc_rank, se_obj)

	if gulag_type == "jupiter_bunker" then
		return npc_community == "stalker"
	end

	if gulag_type == "jupiter_vagon" then
		return npc_community == "stalker"
	end

	if gulag_type == "jup_kpp" then
		return npc_community == "bandit"
	end

	if gulag_type == "jup_sklad" then
		return npc_community == "bandit"
	end

	if gulag_type == "jup_jup" then
		return npc_community == "killer"
	end

	return false
end

