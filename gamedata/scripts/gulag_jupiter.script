function load_states(gname, type)

	if type == "jupiter_bunker" then
		return function(gulag)
			if db.actor ~= nil then
					return 0  -- день
			else
				return gulag.state
			end
		end
	end

	if type == "jupiter_vagon" then
		return function(gulag)
			if db.actor ~= nil then
					return 0  -- день
			else
				return gulag.state
			end
		end
	end

	if type == "jup_kpp" then
		return function(gulag)
			return 0
		end
	end

	if type == "jup_sklad" then
		return function(gulag)
			return 0
		end
	end

	if type == "jup_jup" then
		return function(gulag)
			return 0
		end
	end
end

